<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  <servlet-name>signup</servlet-name>
  <servlet-class>controller.SignUpController</servlet-class>
  </servlet>
  
  
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>controller.LoginController</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>delete</servlet-name>
  <servlet-class>controller.DeleteController</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>details</servlet-name>
  <servlet-class>controller.GetAllDetails</servlet-class>
  </servlet>
  
  <servlet>
  <servlet-name>getbyemail</servlet-name>
  <servlet-class>controller.GetByEmail</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>getbyemail</servlet-name>
  <url-pattern>/getbyemailhtml</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/loginhtml</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  
  <servlet-name>signup</servlet-name>
  <url-pattern>/signuphtml</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>delete</servlet-name>
  <url-pattern>/deletehtml</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  <servlet-name>details</servlet-name>
  <url-pattern>/getalldetails</url-pattern>
  </servlet-mapping>
  
</web-app>
